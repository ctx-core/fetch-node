{
	"name": "@ctx-core/fetch-node",
	"version": "1.1.202",
	"description": "ctx-core isomorphic fetch using node-fetch",
	"keywords": [
		"ctx-core",
		"isomorphic",
		"fetch",
		"node-fetch"
	],
	"homepage": "https://github.com/ctx-core/fetch-node#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fetch-node/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fetch-node.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-fetch-node@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./fetch",
		"./fetch_response_pair",
		"./headers",
		"./Request",
		"./Response"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^7.2.17",
		"node-fetch": "next"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.8.0",
		"tsx": "^4.14.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}